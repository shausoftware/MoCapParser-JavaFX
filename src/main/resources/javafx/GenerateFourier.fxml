<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ButtonType?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<?import javafx.collections.FXCollections?>
<?import java.lang.String?>
<DialogPane prefWidth="540.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.shau.mocap.ui.controller.GenerateFourierController">
   <header>
      <HBox alignment="TOP_CENTER">
         <children>
            <Label text="Generate Fourier Transform" textAlignment="CENTER">
               <font>
                  <Font name="Arial Bold" size="14.0" />
               </font>
            </Label>
         </children>
         <opaqueInsets>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </opaqueInsets>
         <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </padding>
      </HBox>
   </header>
   <buttonTypes>
      <ButtonType fx:constant="CANCEL" />
      <ButtonType fx:constant="OK" />
   </buttonTypes>
   <content>
      <VBox spacing="10.0">
         <children>
            <HBox spacing="10.0">
               <children>
                  <Label prefHeight="26.0" text="Start Frame" />
                  <TextField fx:id="txtStartFrame" prefWidth="50.0" />
                  <Label prefHeight="26.0" text="End Frame" />
                  <TextField fx:id="txtEndFrame" prefWidth="50.0" />
                  <Label prefHeight="26.0" text="Fourier Frames" />
                  <TextField fx:id="txtFourierFrames" prefWidth="50.0" />
               </children>
               <VBox.margin>
                  <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
               </VBox.margin>
            </HBox>
            <HBox spacing="10.0">
               <children>
                  <Label prefHeight="26.0" text="Data Scaling" />
                  <ComboBox fx:id="cmbDataScale" prefWidth="150.0" value="1.0">
                     <items>
                        <FXCollections fx:factory="observableArrayList">
                           <String fx:value="10.0" />
                           <String fx:value="5.0" />
                           <String fx:value="2.0" />
                           <String fx:value="1.0" />
                           <String fx:value="0.5" />
                           <String fx:value="0.25" />
                           <String fx:value="0.1" />
                        </FXCollections>
                     </items>
                  </ComboBox>
               </children>
               <VBox.margin>
                  <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
               </VBox.margin>
            </HBox>
            <HBox spacing="10.0">
               <children>
                  <Label prefHeight="26.0" text="Loop" />
                  <CheckBox fx:id="cbLoop" mnemonicParsing="false" onAction="#cbLoopAction" prefHeight="26.0" />
                  <Label prefHeight="26.0" text="Easing Frames" />
                  <TextField fx:id="txtEasingFrames" prefWidth="50.0" />
               </children>
               <VBox.margin>
                  <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
               </VBox.margin>
            </HBox>
            <HBox spacing="10.0">
               <children>
                  <Label prefHeight="26.0" text="Use Low Resolution" />
                  <CheckBox fx:id="cbUseLowResolution" mnemonicParsing="false" onAction="#cbUseLowResolutionAction" prefHeight="26.0" />
                  <Label prefHeight="26.0" text="Cutoff" />
                  <TextField fx:id="txtCutoff" prefWidth="50.0" />
               </children>
               <VBox.margin>
                  <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </content>
</DialogPane>
